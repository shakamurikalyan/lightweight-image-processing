{
    "compilerOptions": {
      "incremental": true,
      "outDir": "./dist/",
      "target": "ESNext",
      "module": "NodeNext",
      "allowJs": true,
      "resolveJsonModule": true,
      "declaration": true,
      "sourceMap": true,
      "composite": true,
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "./src/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  